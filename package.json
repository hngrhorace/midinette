{
  "name": "midinette",
  "version": "1.0.5",
  "description": "MIDI utility library",
  "main": "dist/index.js",
  "jsnext:main": "src/index.js",
  "scripts": {
    "build:babel": "babel src --out-dir dist",
    "build:browserify": "browserify dist/*.js --outfile bundle/midinette.js",
    "build": "npm run build:babel && npm run build:browserify",
    "test": "mocha tests/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hngrhorace/midinette.git"
  },
  "keywords": [
    "midi",
    "util"
  ],
  "author": "Andor Polgar",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hngrhorace/midinette/issues"
  },
  "homepage": "https://github.com/hngrhorace/midinette#readme",
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-preset-es2015": "^6.5.0",
    "browserify": "^13.0.0",
    "mocha": "^2.4.5",
    "underscore": "^1.8.3"
  }
}
